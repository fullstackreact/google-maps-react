    "cssnano": "^3.5.2",
    "dotenv": "^2.0.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.8.5",
